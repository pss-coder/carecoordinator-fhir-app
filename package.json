    "fhir:start-server": "docker start carecoordinator-fhir-app_fhir_server || docker run -p 8100:8100 -p 8103:8103 --name carecoordinator-fhir-app_fhir_server -e INITIAL_CLIENT_APP_REDIRECT_URI=http://localhost:3000/api/auth/callback/medplum ghcr.io/bonfhir/medplum-devbox:latest",
